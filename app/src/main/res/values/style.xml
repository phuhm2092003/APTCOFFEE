<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">
    <!--  Theme Status Bar white  -->
    <style name="AppStatusBarWhite" parent="Theme.APTCOFFEE">
        <item name="android:statusBarColor">@color/WhitePrimary</item>
        <item name="android:windowLightStatusBar" tools:ignore="NewApi">true</item>
    </style>
    <!--  Theme Status Bar Gray -->
    <style name="AppStatusBarGray" parent="Theme.APTCOFFEE">
        <item name="android:statusBarColor">@color/GrayPrimary</item>
        <item name="android:windowLightStatusBar" tools:ignore="NewApi">true</item>
    </style>
    <style name="BottomNavigationViewTextStyle">
        <item name="android:fontFamily">@font/inter_regular</item>
        <item name="android:textSize">12sp</item>
    </style>
</resources>